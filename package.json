{
  "name": "ellipsis-html",
  "version": "1.1.3",
  "description": "Mini tool to align HTML elements along an ellipse or a part of an ellipse",
  "main": "lib/ellipsis-html.cjs.js",
  "module": "lib/ellipsis-html.esm.js",
  "browser": "lib/ellipsis-html.min.js",
  "license": "MIT",
  "author": "Behnam Azimi",
  "homepage": "https://github.com/behnamazimi/ellipsis-html",
  "repository": {
    "type": "git",
    "url": "https://github.com/behnamazimi/ellipsis-html.git"
  },
  "bugs": {
    "url": "https://github.com/behnamazimi/ellipsis-html/issues"
  },
  "scripts": {
    "dev": "rollup -c rollup.config.dev.js -w",
    "build": "rollup -c rollup.config.js"
