{
  "name": "html-ellipsis",
  "version": "0.0.3",
  "description": "Mini tool to align HTML elements along an ellipse or a part of an ellipse",
  "main": "lib/html-ellipsis.cjs.js",
  "module": "lib/html-ellipsis.esm.js",
  "browser": "lib/html-ellipsis.min.js",
  "license": "MIT",
  "author": "Behnam Azimi",
  "homepage": "https://github.com/behnamazimi/html-ellipsis",
  "repository": {
    "type": "git",
    "url": "https://github.com/behnamazimi/html-ellipsis.git"
  },
  "bugs": {
    "url": "https://github.com/behnamazimi/html-ellipsis/issues"
  },
  "scripts": {
    "dev": "rollup -c rollup.config.dev.js -w",
    "build": "rollup -c rollup.config.js"
  },
  "keywords": [
    "curved html elements",
    "elements along ellipse",
